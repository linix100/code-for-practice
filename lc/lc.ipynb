   "source": [
    "class Solutions:\n",
    "    def twoSum(self, nums, target):\n",
    "\t    mapping={}\n",
    "\t    for i,number1 in enumerate(nums):\n",
    "\t        number2=target-number1\n",
    "\t        if number2 in mapping:\n",
    "\t            return [i,mapping[number2]]\n",
    "\t        else:\n",
    "\t            mapping[number1]=i"
   ]
